{
    "Logging": {
        "LogLevel": {
            "Microsoft.Hosting.Lifetime": "None"
        }
    },
    "RabbitMQOptions": {
        "Host": "localhost",
        "Port": 5672,
        "ClientName": "asgard-client"
    },
    "RabbitMQSubscriptionOptions": {
        "Configurations": [
          {
            "Queue": "queue.main",
            "QueueArguments": {
              "x-queue-type": "quorum",
              "x-dead-letter-exchange": "ex.dead"
            },
            "Exchange": "ex.harleydikkinson",
            "RetryExchange": "ex.retry",
            "DeadLetterExchange": "ex.dead",
            "DeadLetterQueue": "queue.dead",
            "RetryQueue": "queue.retry",
            "Publish": [
              {
                "Key": "pub-asgard-user",
                "RoutingKey": "asgard-user"
              },
              {
                "Key": "pub-asgard-person",
                "RoutingKey": "asgard-person"
              }
            ],
            "Bindings": [
              {
                "RoutingKey": "asgard-user",
                "Retry": {
                  "MaxRetries": 2,
                  "DelaysSeconds": [ 2, 4 ]
                }
              },
              {
                "RoutingKey": "asgard-person",
                "Retry": {
                  "MaxRetries": 3,
                  "DelaysSeconds": [ 3, 5, 3 ]
                }
              }
            ]
          }
        ]
    }
}
