{
    "Logging": {
        "LogLevel": {
            "Microsoft.Hosting.Lifetime": "None"
        }
    },
    "RabbitMQOptions": {
        "Host": "localhost",
        "Port": 5672,
        "ClientName": "asgard-client"
    },
    "RabbitMQSubscriptionOptions": {
        "Configurations": [
            {
                "Queue": "queue.main",
                "Exchange": "ex.test",
                "RetryExchange": "ex.retry",
                "DeadLetterExchange": "ex.dead",
                "DeadLetterQueue": "queue.dead",
                "RetryQueue": "queue.retry",
                "Bindings": [
                    {
                        "RoutingKey": "user",
                        "Retry": {
                            "MaxRetries": 2,
                            "DelaysSeconds": [ 2, 4 ]
                        }
                    },
                    {
                        "RoutingKey": "person",
                        "Retry": {
                            "MaxRetries": 3,
                            "DelaysSeconds": [ 3, 5, 3 ]
                        }
                    }
                ]
            }
        ]
    }
}
